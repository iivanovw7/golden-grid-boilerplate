{
  "name": "landing-grids-postcss",
  "version": "1.0.0",
  "description": "Portfolio website made with pug and postcss",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --verbose --colors",
    "coverage": "npm test -- --coverage",
    "start": "cd server && go run server.go",
    "pre-commit": "run-s \"clean\" \"stylelint\" \"eslint\"",
    "pre-push": "run-s \"clean\" \"stylelint\" \"eslint\"",
    "dev": "run-s \"stylelint\" \"dev:server\"",
    "dev:dashboard": "run-s \"stylelint\" \"eslint\" \"dev:server:dashboard\"",
    "dev:server": "webpack-dev-server --mode development -d --hot --config webpack/webpack.dev.js --watch",
    "dev:server:dashboard": "webpack-dashboard -- webpack-dev-server --mode development -d --hot --config webpack/webpack.dev.js --watch --open",
    "build": "run-s \"stylelint\" \"clean\" \"build:webpack\" \"build:gulp\"",
    "build:gulp": "gulp",
    "build:webpack": "webpack --mode production --config webpack/webpack.prod.js",
    "reset": "rm -rf dist && rm -rf node_modules",
    "clean": "rm -rf dist",
    "stylelint": "stylelint './src/styles/**/*.pcss'",
    "eslint": "eslint .",
    "doc": "jsdoc -c config/jsdoc/conf.json",
    "prettify": "prettier --write"
  },
  "author": "iivanovw7",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@svgr/webpack": "^4.3.3",
    "autoprefixer": "^9.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "better-docs": "^1.4.7",
    "chalk": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-fetch": "^3.0.4",
    "css-color-converter": "^1.1.0",
    "css-loader": "^3.3.2",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-guard": "^1.0.3",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "^7.16.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "fs": "0.0.1-security",
    "gulp": "^4.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^24.9.0",
    "jest-transform-pug": "^0.1.0",
    "jsdoc": "^3.6.3",
    "mini-css-extract-plugin": "^0.8.0",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.2.10",
